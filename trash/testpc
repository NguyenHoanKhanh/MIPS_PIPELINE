#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_012AD7B8 .scope module, "tb" "tb" 2 3;
 .timescale 0 0;
v012AEE40_0 .var "pc_clk", 0 0;
v012AEE98_0 .var "pc_i_ce", 0 0;
v012AEEF0_0 .var "pc_i_change_pc", 0 0;
v012AEF48_0 .var "pc_i_pc", 31 0;
v012AE9F0_0 .net "pc_o_ce", 0 0, v012AAF08_0; 1 drivers
v012AEA48_0 .net "pc_o_pc", 31 0, v012AAF60_0; 1 drivers
v012AEAA0_0 .var "pc_rst", 0 0;
S_012ACA70 .scope task, "reset" "reset" 2 31, 2 31, S_012AD7B8;
 .timescale 0 0;
v012A3368_0 .var/i "counter", 31 0;
E_012AC458 .event posedge, v012A43D0_0;
TD_tb.reset ;
    %set/v v012AEAA0_0, 0, 1;
    %load/v 8, v012A3368_0, 32;
T_0.0 %cmp/s 0, 8, 32;
    %jmp/0xz T_0.1, 5;
    %add 8, 1, 32;
    %wait E_012AC458;
    %jmp T_0.0;
T_0.1 ;
    %set/v v012AEAA0_0, 1, 1;
    %end;
S_012AD950 .scope module, "pc" "prog_counter" 2 11, 3 4, S_012AD7B8;
 .timescale 0 0;
v012A43D0_0 .net "pc_clk", 0 0, v012AEE40_0; 1 drivers
v012A4428_0 .net "pc_i_ce", 0 0, v012AEE98_0; 1 drivers
v012A4480_0 .net "pc_i_change_pc", 0 0, v012AEEF0_0; 1 drivers
v012AAEB0_0 .net "pc_i_pc", 31 0, v012AEF48_0; 1 drivers
v012AAF08_0 .var "pc_o_ce", 0 0;
v012AAF60_0 .var "pc_o_pc", 31 0;
v012A32B8_0 .net "pc_rst", 0 0, v012AEAA0_0; 1 drivers
v012A3310_0 .var "temp_pc", 31 0;
E_012AC438/0 .event negedge, v012A32B8_0;
E_012AC438/1 .event posedge, v012A43D0_0;
E_012AC438 .event/or E_012AC438/0, E_012AC438/1;
    .scope S_012AD950;
T_1 ;
    %wait E_012AC438;
    %load/v 8, v012A32B8_0, 1;
    %inv 8, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/load 0, 32, 0;
    %assign/v0 v012AAF60_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v012AAF08_0, 0, 0;
    %ix/load 0, 32, 0;
    %assign/v0 v012A3310_0, 0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v012A4428_0, 1;
    %jmp/0xz  T_1.2, 8;
    %load/v 8, v012A3310_0, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v012AAF60_0, 0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v012AAF08_0, 0, 1;
    %load/v 8, v012A4480_0, 1;
    %jmp/0xz  T_1.4, 8;
    %load/v 8, v012AAEB0_0, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v012A3310_0, 0, 8;
    %jmp T_1.5;
T_1.4 ;
    %load/v 8, v012A3310_0, 32;
    %mov 40, 0, 1;
    %addi 8, 4, 33;
    %ix/load 0, 32, 0;
    %assign/v0 v012A3310_0, 0, 8;
T_1.5 ;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_012AD7B8;
T_2 ;
    %set/v v012AEE40_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_012AD7B8;
T_3 ;
    %delay 5, 0;
    %load/v 8, v012AEE40_0, 1;
    %inv 8, 1;
    %set/v v012AEE40_0, 8, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_012AD7B8;
T_4 ;
    %vpi_call 2 27 "$dumpfile", "./waveform/pc.vcd";
    %vpi_call 2 28 "$dumpvars", 1'sb0, S_012AD7B8;
    %end;
    .thread T_4;
    .scope S_012AD7B8;
T_5 ;
    %movi 8, 2, 32;
    %set/v v012A3368_0, 8, 32;
    %fork TD_tb.reset, S_012ACA70;
    %join;
    %wait E_012AC458;
    %set/v v012AEE98_0, 1, 1;
    %movi 8, 20, 6;
T_5.0 %cmp/s 0, 8, 6;
    %jmp/0xz T_5.1, 5;
    %add 8, 1, 6;
    %wait E_012AC458;
    %jmp T_5.0;
T_5.1 ;
    %delay 200, 0;
    %vpi_call 2 48 "$finish";
    %end;
    .thread T_5;
    .scope S_012AD7B8;
T_6 ;
    %vpi_call 2 52 "$monitor", $time, " ", "pc_o_ce = %b, pc_o_pc = %d", v012AE9F0_0, v012AEA48_0;
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
