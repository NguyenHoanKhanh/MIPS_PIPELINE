v01200728_0 .var "m_rst", 0 0;
v012005C8_0 .var "m_wr_en", 0 0;
S_00A99C18 .scope task, "display" "display" 2 63, 2 63, S_00A99A80;
 .timescale 0 0;
v00A9AEB0_0 .var/i "counter", 31 0;
TD_tb.display ;
    %set/v v01200780_0, 1, 1;
    %set/v v00A9AF60_0, 0, 32;
T_0.0 ;
    %load/v 8, v00A9AF60_0, 32;
    %load/v 40, v00A9AEB0_0, 32;
    %cmp/s 8, 40, 32;
    %jmp/0xz T_0.1, 5;
    %wait E_00A971E0;
    %load/v 8, v00A9AF60_0, 32;
    %set/v v00A9AF08_0, 8, 32;
    %wait E_00A971E0;
    %vpi_call 2 70 "$display", $time, " ", "load addr = %d, load data = %d", v00A9AF08_0, v012008E0_0;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v00A9AF60_0, 32;
    %set/v v00A9AF60_0, 8, 32;
    %jmp T_0.0;
T_0.1 ;
    %wait E_00A971E0;
    %set/v v01200780_0, 0, 1;
    %end;
S_00A9A740 .scope task, "load_data" "load_data" 2 50, 2 50, S_00A99A80;
 .timescale 0 0;
v00A9AE58_0 .var/i "counter", 31 0;
TD_tb.load_data ;
    %set/v v012005C8_0, 1, 1;
    %set/v v00A9AF60_0, 0, 32;
T_1.2 ;
    %load/v 8, v00A9AF60_0, 32;
    %load/v 40, v00A9AE58_0, 32;
    %cmp/s 8, 40, 32;
    %jmp/0xz T_1.3, 5;
    %wait E_00A971E0;
    %load/v 8, v00A9AF60_0, 32;
    %set/v v00A9AF08_0, 8, 32;
    %load/v 8, v00A9AF60_0, 32;
    %set/v v01200A98_0, 8, 32;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v00A9AF60_0, 32;
    %set/v v00A9AF60_0, 8, 32;
    %jmp T_1.2;
T_1.3 ;
    %wait E_00A971E0;
    %set/v v012005C8_0, 0, 1;
    %end;
S_00A9A6B8 .scope task, "reset" "reset" 2 42, 2 42, S_00A99A80;
 .timescale 0 0;
v00A942C0_0 .var/i "counter", 31 0;
E_00A971E0 .event posedge, v00A93490_0;
TD_tb.reset ;
    %set/v v01200728_0, 0, 1;
    %load/v 8, v00A942C0_0, 32;
T_2.4 %cmp/s 0, 8, 32;
    %jmp/0xz T_2.5, 5;
    %add 8, 1, 32;
    %wait E_00A971E0;
    %jmp T_2.4;
T_2.5 ;
    %set/v v01200728_0, 1, 1;
    %end;
S_00A9A5A8 .scope module, "m" "memory" 2 17, 3 4, S_00A99A80;
 .timescale 0 0;
P_00A9E544 .param/l "AWIDTH_MEM" 3 6, +C4<0100000>;
P_00A9E558 .param/l "DWIDTH" 3 5, +C4<0100000>;
v01202028_0 .net "alu_value_addr", 31 0, v00A9AF08_0; 1 drivers
v00A9ACD8 .array "data_mem", 31 0, 31 0;
v00A9B868_0 .var/i "i", 31 0;
v00A93490_0 .net "m_clk", 0 0, v00A9AFB8_0; 1 drivers
v00A934E8_0 .net "m_i_ce", 0 0, v00A9B010_0; 1 drivers
v00A93540_0 .net "m_i_store_data", 31 0, v01200A98_0; 1 drivers
v00A93598_0 .var "m_o_load_data", 31 0;
v00A941B8_0 .net "m_rd_en", 0 0, v01200780_0; 1 drivers
v00A94210_0 .net "m_rst", 0 0, v01200728_0; 1 drivers
v00A94268_0 .net "m_wr_en", 0 0, v012005C8_0; 1 drivers
E_00A973E0/0 .event negedge, v00A94210_0;
E_00A973E0/1 .event posedge, v00A93490_0;
E_00A973E0 .event/or E_00A973E0/0, E_00A973E0/1;
    .scope S_00A9A5A8;
T_3 ;
    %wait E_00A973E0;
    %load/v 8, v00A94210_0, 1;
    %inv 8, 1;
    %jmp/0xz  T_3.0, 8;
    %set/v v00A9B868_0, 0, 32;
T_3.2 ;
    %load/v 8, v00A9B868_0, 32;
   %cmpi/s 8, 32, 32;
    %jmp/0xz T_3.3, 5;
    %ix/getv/s 3, v00A9B868_0;
    %jmp/1 t_0, 4;
    %ix/load 0, 32, 0; word width
    %ix/load 1, 0, 0; part off
    %assign/av v00A9ACD8, 0, 0;
t_0 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v00A9B868_0, 32;
    %set/v v00A9B868_0, 8, 32;
    %jmp T_3.2;
T_3.3 ;
    %ix/load 0, 32, 0;
    %assign/v0 v00A93598_0, 0, 0;
    %jmp T_3.1;
T_3.0 ;
    %load/v 8, v00A934E8_0, 1;
    %jmp/0xz  T_3.4, 8;
    %load/v 8, v00A94268_0, 1;
    %jmp/0xz  T_3.6, 8;
    %load/v 8, v00A93540_0, 32;
    %ix/getv 3, v01202028_0;
    %jmp/1 t_1, 4;
    %ix/load 0, 32, 0; word width
    %ix/load 1, 0, 0; part off
    %assign/av v00A9ACD8, 0, 8;
t_1 ;
T_3.6 ;
    %load/v 8, v00A941B8_0, 1;
    %jmp/0xz  T_3.8, 8;
    %ix/getv 3, v01202028_0;
    %load/av 8, v00A9ACD8, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v00A93598_0, 0, 8;
T_3.8 ;
T_3.4 ;
T_3.1 ;
    %jmp T_3;
    .thread T_3;
    .scope S_00A99A80;
T_4 ;
    %set/v v00A9B010_0, 0, 1;
    %set/v v012005C8_0, 0, 1;
    %set/v v01200780_0, 0, 1;
    %set/v v00A9AFB8_0, 0, 1;
    %set/v v00A9AF60_0, 0, 32;
    %end;
    .thread T_4;
    .scope S_00A99A80;
T_5 ;
    %delay 5, 0;
    %load/v 8, v00A9AFB8_0, 1;
    %inv 8, 1;
    %set/v v00A9AFB8_0, 8, 1;
    %jmp T_5;
