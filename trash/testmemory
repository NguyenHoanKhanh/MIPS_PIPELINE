    .scope S_00A99A80;
T_6 ;
    %vpi_call 2 38 "$dumpfile", "./waveform/memory.vcd";
    %vpi_call 2 39 "$dumpvars", 1'sb0, S_00A99A80;
    %end;
    .thread T_6;
    .scope S_00A99A80;
T_7 ;
    %movi 8, 2, 32;
    %set/v v00A942C0_0, 8, 32;
    %fork TD_tb.reset, S_00A9A6B8;
    %join;
    %set/v v00A9B010_0, 1, 1;
    %wait E_00A971E0;
    %movi 8, 10, 32;
    %set/v v00A9AE58_0, 8, 32;
    %fork TD_tb.load_data, S_00A9A740;
    %join;
    %wait E_00A971E0;
    %movi 8, 10, 32;
    %set/v v00A9AEB0_0, 8, 32;
    %fork TD_tb.display, S_00A99C18;
    %join;
    %wait E_00A971E0;
    %set/v v00A9B010_0, 0, 1;
    %delay 20, 0;
    %vpi_call 2 86 "$finish";
    %end;
    .thread T_7;
