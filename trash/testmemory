S_00A99A80 .scope module, "tb" "tb" 2 3;
 .timescale 0 0;
P_00A9E81C .param/l "AWIDTH_MEM" 2 5, +C4<0100000>;
P_00A9E830 .param/l "DWIDTH" 2 4, +C4<0100000>;
v00A9AF08_0 .var "alu_value_addr", 31 0;
