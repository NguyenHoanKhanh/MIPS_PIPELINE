#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_011FD190 .scope module, "tb" "tb" 2 3;
 .timescale 0 0;
v011F4E28_0 .var "pc_clk", 0 0;
v011F4E80_0 .var "pc_i_ce", 0 0;
v011FEDF0_0 .var "pc_i_change_pc", 0 0;
v011FEE48_0 .var "pc_i_pc", 31 0;
v011FEEA0_0 .net "pc_o_ce", 0 0, v011FE768_0; 1 drivers
v011FEEF8_0 .net "pc_o_pc", 31 0, v01232808_0; 1 drivers
v011FEF50_0 .var "pc_rst", 0 0;
S_011FCB30 .scope task, "reset" "reset" 2 31, 2 31, S_011FD190;
 .timescale 0 0;
v011F4DD0_0 .var/i "counter", 31 0;
E_011FC7F0 .event posedge, v011FE5C8_0;
TD_tb.reset ;
    %set/v v011FEF50_0, 0, 1;
    %load/v 8, v011F4DD0_0, 32;
T_0.0 %cmp/s 0, 8, 32;
    %jmp/0xz T_0.1, 5;
    %add 8, 1, 32;
    %wait E_011FC7F0;
    %jmp T_0.0;
T_0.1 ;
    %set/v v011FEF50_0, 1, 1;
    %end;
S_011FCA20 .scope module, "pc" "prog_counter" 2 11, 3 4, S_011FD190;
 .timescale 0 0;
v011FE5C8_0 .net "pc_clk", 0 0, v011F4E28_0; 1 drivers
v011FE620_0 .net "pc_i_ce", 0 0, v011F4E80_0; 1 drivers
v011FE6B8_0 .net "pc_i_change_pc", 0 0, v011FEDF0_0; 1 drivers
v011FE710_0 .net "pc_i_pc", 31 0, v011FEE48_0; 1 drivers
v011FE768_0 .var "pc_o_ce", 0 0;
v01232808_0 .var "pc_o_pc", 31 0;
v01232860_0 .net "pc_rst", 0 0, v011FEF50_0; 1 drivers
v012328B8_0 .var "temp_pc", 31 0;
E_011FC530/0 .event negedge, v01232860_0;
E_011FC530/1 .event posedge, v011FE5C8_0;
E_011FC530 .event/or E_011FC530/0, E_011FC530/1;
    .scope S_011FCA20;
T_1 ;
    %wait E_011FC530;
    %load/v 8, v01232860_0, 1;
    %inv 8, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v011FE768_0, 0, 0;
    %ix/load 0, 32, 0;
    %assign/v0 v01232808_0, 0, 0;
    %ix/load 0, 32, 0;
    %assign/v0 v012328B8_0, 0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v011FE620_0, 1;
    %jmp/0xz  T_1.2, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v011FE768_0, 0, 1;
    %load/v 8, v012328B8_0, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v01232808_0, 0, 8;
    %load/v 8, v011FE6B8_0, 1;
    %jmp/0xz  T_1.4, 8;
    %load/v 8, v011FE710_0, 32;
    %ix/load 0, 32, 0;
    %assign/v0 v012328B8_0, 0, 8;
    %jmp T_1.5;
T_1.4 ;
    %load/v 8, v012328B8_0, 32;
    %mov 40, 0, 1;
    %addi 8, 4, 33;
    %ix/load 0, 32, 0;
    %assign/v0 v012328B8_0, 0, 8;
T_1.5 ;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_011FD190;
T_2 ;
    %set/v v011F4E28_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_011FD190;
T_3 ;
    %delay 5, 0;
    %load/v 8, v011F4E28_0, 1;
    %inv 8, 1;
    %set/v v011F4E28_0, 8, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_011FD190;
T_4 ;
    %vpi_call 2 27 "$dumpfile", "./waveform/pc.vcd";
    %vpi_call 2 28 "$dumpvars", 1'sb0, S_011FD190;
    %end;
    .thread T_4;
    .scope S_011FD190;
T_5 ;
    %movi 8, 2, 32;
    %set/v v011F4DD0_0, 8, 32;
    %fork TD_tb.reset, S_011FCB30;
    %join;
    %wait E_011FC7F0;
    %set/v v011F4E80_0, 1, 1;
    %movi 8, 20, 6;
T_5.0 %cmp/s 0, 8, 6;
    %jmp/0xz T_5.1, 5;
    %add 8, 1, 6;
    %wait E_011FC7F0;
    %jmp T_5.0;
T_5.1 ;
    %delay 20, 0;
    %vpi_call 2 48 "$finish";
    %end;
    .thread T_5;
    .scope S_011FD190;
T_6 ;
    %vpi_call 2 52 "$monitor", $time, " ", "pc_o_ce = %b, pc_o_pc = %d", v011FEEA0_0, v011FEEF8_0;
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
